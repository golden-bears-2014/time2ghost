<div class="content">

  <div class="notice">
    <% if flash[:error] %>
      <p class="error"><%= flash[:error]  %></p>
    <% end %>
  </div>

  <%= form_for @user do |f| %>
    <%= f.label :username %>:
    <%= f.text_field :username %><br />

    <%= f.label :password %>:
    <%= f.text_field :password %><br />

    <%= f.label :phone_number %>:
    <%= f.text_field :phone_number %><br />

    <%= f.label :email %>:
    <%= f.text_field :email %><br />

    <%= f.submit %>
  <% end %>


</div>


